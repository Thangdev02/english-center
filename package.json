{
  "name": "education-english",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run tailwind:watch\" \"vite\" \"npm run server\"",
    "server": "json-server --watch db.json --port 3001",
    "build": "npm run tailwind:build && vite build",
    "preview": "vite preview",
    "tailwind:watch": "tailwindcss -i ./src/input.css -o ./src/output.css --watch",
    "tailwind:build": "tailwindcss -i ./src/input.css -o ./src/output.css --minify",
    "serve": "node api/server.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "antd": "^5.27.4",
    "autoprefixer": "^10.4.21",
    "axios": "^1.12.2",
    "dayjs": "^1.11.18",
    "framer-motion": "^12.23.22",
    "json-server": "^1.0.0-beta.3",
    "lucide-react": "^0.544.0",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.64.0",
    "react-quill": "^2.0.0",
    "react-router-dom": "^7.9.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "tailwindcss": "^3.4.18",
    "vite": "^7.1.7"
  }
}
